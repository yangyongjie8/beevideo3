            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="com.skyworthdigital.voice.dingdang.fmplay" />
            </intent-filter>
        </activity>
        <activity
            android:name=".domains.sports.SportsScheduleActivity"
