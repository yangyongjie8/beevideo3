        <activity android:name=".domains.recipe.RecipeActivity">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="com.skyworthdigital.voice.dingdang.recipe" />
            </intent-filter>
        </activity>


        <activity
            android:name=".train.TrainTicketActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="com.skyworthdigital.voice.dingdang.trainticket" />
            </intent-filter>
        </activity>
    </application>
</manifest>
