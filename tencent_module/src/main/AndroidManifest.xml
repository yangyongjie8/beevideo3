<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.skyworthdigital.voice.tencent_module" >
    <application>
        <activity
            android:name="com.skyworthdigital.voice.tencent_module.domains.poem.SkyPoemActivity"
